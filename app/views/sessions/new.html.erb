<table align=center>
<tr>

<td width=200>

<img src="./LeftFacing.gif">

</td>

<td>

<h2>Log-in</h2>

<br>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>
<br><br>
      <%= f.label :password %>
      <%= f.password_field :password %>
<br><br>
      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>

</td>
</tr>
</table>
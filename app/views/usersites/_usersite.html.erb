<h1>Listing usersites</h1>

<table>
  <tr>
    <th>Maxscheduler</th>
    <th>Site</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @usersites.each do |usersite| %>
  <tr>
    <td><%= usersite.maxscheduler_id %></td>
    <td><%= usersite.site_id %></td>
    <td><%= usersite.user_id %></td>
    <td><%= link_to 'Show', usersite %></td>
    <td><%= link_to 'Edit', edit_usersite_path(usersite) %></td>
    <td><%= link_to 'Destroy', usersite, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

